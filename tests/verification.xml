	<global>
		<deviceID>0</deviceID>
		<devicememsize>1.75</devicememsize>
	</global>
	<testrun>
		<configuration>
			<memorylayout>AOS</memorylayout>
			<graphmode>simple</graphmode>
			<updatevariant>standard</updatevariant>
			<rounds>5</rounds>
